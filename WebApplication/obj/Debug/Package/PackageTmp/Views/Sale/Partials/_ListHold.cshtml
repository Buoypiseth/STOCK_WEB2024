@if (ViewBag.holdCount > 0)
{
    <ul class="list-group list-group-flush">
        @foreach (var i in ViewBag.GetCustHold)
        {
            <li class="list-group-item showHold" data-id=@i.roomtableID>@i.CustomerName</li>
        }
    </ul>
}
else
{
    <div class="align-self-center">
        <div class="p-2 text-center text-muted">No data available!</div>
    </div>
}